{
  "name": "infracopilot-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.2.1",
    "@azure/communication-react": "==1.13.0-beta.1",
    "@hookform/error-message": "^2.0.1",
    "@segment/analytics-next": "^1.59.0",
    "@uiw/react-md-editor": "^4.0.3",
    "ace-builds": "^1.32.5",
    "autoprefixer": "^10.4.17",
    "axios": "^1.5.0",
    "brace": "^0.11.1",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "deep-equal": "^2.2.3",
    "elkjs": "^0.8.2",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "html-to-image": "^1.11.11",
    "jszip": "^3.10.1",
    "lodash.debounce": "^4.0.8",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-ace-builds": "^7.4.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-hook-form": "^7.50.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "reactflow": "^11.10.4",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "yaml": "^2.3.1",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@swc/cli": "^0.3.6",
    "@swc/core": "^1.3.107",
    "@testing-library/jest-dom": "^6.4.1",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/ace": "^0.0.52",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^20.11.14",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/sortablejs": "^1.15.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "env-cmd": "^10.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vitest": "^0.3.21",
    "flowbite-typography": "^1.0.3",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.70.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vitest": "^1.2.2",
    "web-worker": "^1.3.0"
  },
  "overrides": {
    "react-scripts": {
      "react-refresh": "^0.14.0"
    }
  },
  "scripts": {
    "start": "PORT=3100 react-scripts start",
    "build": "react-scripts build",
    "build-dev": "env-cmd -f ./.env.dev npm run build",
    "build-prod": "env-cmd -f ./.env.prod npm run build",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "eject": "react-scripts eject",
    "clean": "rm -rf ./build",
    "fixup": "npx prettier --write . && npx eslint . --fix"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vitest/recommended",
      "eslint:recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:tailwindcss/recommended",
      "react-app"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "vitest",
      "unused-imports"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "tailwindcss/classnames-order": [
        "error",
        {
          "callees": [
            "classnames"
          ],
          "config": "./tailwind.config.js"
        }
      ],
      "tailwindcss/no-custom-classname": [
        "off",
        {
          "config": "./tailwind.config.js"
        }
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3000",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,ejs,cjs,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  }
}
