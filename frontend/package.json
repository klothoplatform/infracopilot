{
  "name": "infracopilot-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@azure/communication-react": "==1.13.0-beta.1",
    "@hookform/error-message": "^2.0.1",
    "@rudderstack/analytics-js": "^3.6.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@uiw/react-md-editor": "^4.0.4",
    "ace-builds": "^1.35.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "brace": "^0.11.1",
    "chonky": "^2.3.2",
    "chonky-icon-fontawesome": "^2.3.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "deep-equal": "^2.2.3",
    "elkjs": "^0.9.3",
    "flowbite": "^2.2.1",
    "flowbite-react": "^0.7.2",
    "html-to-image": "^1.11.11",
    "jszip": "^3.10.1",
    "linkify-react": "^4.1.3",
    "lodash.debounce": "^4.0.8",
    "react": "^18.3.1",
    "react-ace": "^12.0.0",
    "react-ace-builds": "^7.4.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.52.1",
    "react-hotkeys-hook": "^4.5.0",
    "react-icons": "^5.2.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.25.1",
    "react-syntax-highlighter": "^15.5.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "tailwindcss": "^3.4.6",
    "uuid": "^10.0.0",
    "web-vitals": "^4.2.2",
    "yaml": "^2.4.5",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/ace": "^0.0.52",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.8",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vitest": "^0.4.1",
    "flowbite-typography": "^1.0.3",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.77.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.0.3",
    "web-worker": "^1.3.0"
  },
  "overrides": {
    "react-scripts": {
      "react-refresh": "^0.14.0"
    }
  },
  "scripts": {
    "start": "vite --port 3100",
    "build": "tsc && vite build",
    "preview": "vite preview --port 3100",
    "serve:local": "npm run build:local && vite preview --mode --port 3100",
    "serve:dev": "npm run build:dev && vite preview --mode production --port 3100",
    "serve:prod": "npm run build:prod && vite preview --mode production --port 3100",
    "build:local": "env-cmd -f ./.env.local npm run build",
    "build:dev": "env-cmd -f ./.env.dev npm run build",
    "build:prod": "env-cmd -f ./.env.prod npm run build",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "clean": "rm -rf ./dist",
    "fixup": "npx prettier --write . && npx eslint . --fix",
    "reinit": "rm -rf node_modules && rm -rf package-lock.json && npm install",
    "visualize": "npx vite-bundle-visualizer",
    "visualize:sunburst": "npx vite-bundle-visualizer -t sunburst",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vitest/recommended",
      "eslint:recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:tailwindcss/recommended",
      "react-app"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "project": "./tsconfig.json"
    },
    "plugins": [
      "vitest",
      "unused-imports",
      "react-refresh",
      "react",
      "react-hooks"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/consistent-type-imports": "error",
      "react/react-in-jsx-scope": "off",
      "react/no-unescaped-entities": "off",
      "tailwindcss/classnames-order": [
        "error",
        {
          "callees": [
            "classnames"
          ],
          "config": "./tailwind.config.js"
        }
      ],
      "tailwindcss/no-custom-classname": [
        "off",
        {
          "config": "./tailwind.config.js"
        }
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports": "error",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://127.0.0.1:3000",
  "lint-staged": {
    "*.{ts,tsx,js,jsx,ejs,cjs,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6",
    "@swc/core": "^1.4.12",
    "@swc/core-darwin-arm64": "^1.4.12",
    "@swc/core-darwin-x64": "^1.4.12",
    "@swc/core-linux-arm-gnueabihf": "^1.4.12",
    "@swc/core-linux-arm64-gnu": "^1.4.12",
    "@swc/core-linux-arm64-musl": "^1.4.12",
    "@swc/core-linux-x64-gnu": "^1.4.12",
    "@swc/core-linux-x64-musl": "^1.4.12",
    "@swc/core-win32-arm64-msvc": "^1.4.12",
    "@swc/core-win32-ia32-msvc": "^1.4.12",
    "@swc/core-win32-x64-msvc": "^1.4.12"
  }
}
